{
  "name": "restoration-calendar",
  "version": "1.0.0",
  "description": "Restoration calendar application",
  "main": "main.js",
  "scripts": {
    "start": "electron ./main.js",
    "build": "electron-builder --win --ia32"
  },
  "build-win": "electron-packager . --platform=win32 --asar --prune --arch=ia32 --extra-resource=./extraResources/* --icon=./icon.ico --out ./dist --overwrite",
  "repository": "https://github.com/HappyFuther/Electron-Custom-Calendar",
  "keywords": [
    "Electron",
    "Custom",
    "Calendar",
    "Restoration",
    "Notification"
  ],
  "author": "Genera",
  "license": "CC0-1.0",
  "devDependencies": {
    "@electron-forge/cli": "^6.2.1",
    "electron": "^25.3.2",
    "electron-builder": "^24.6.3",
    "electron-reload": "^2.0.0-alpha.1",
    "electron-reloader": "^1.2.3"
  },
  "dependencies": {
    "@electron/remote": "^2.0.10",
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.1",
    "jquery": "^3.7.0",
    "node-notifier": "^10.0.1",
    "sqlite3": "^5.1.6"
  }
}
